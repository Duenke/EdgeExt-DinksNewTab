{
	"name": "Dinks New Tab",
	"description": "A new tab replacement to keep your bookmarks in sync with your new tab!",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": [
		"bookmarks",
		"storage"
	],
	"chrome_url_overrides": {
		"newtab": "./index.html"
	},
	"action": {
		"default_title": "Dinks New Tab",
		"default_icon": "favicon.png"
	},
	"icons": {
		"512": "favicon.png"
	}
}